<% @users.each_with_index do |user, index| %>
  <tr>
    <td>
      #<%= current_index Settings.per_page, index,
        @users.current_page %>
    </td>
    <td><%= user.name %></td>
    <td><%= user.email %></td>
    <td>
      <% unless current_user.is_user? user %>
        <%= link_to t("delete"), admin_user_path(user.id), method: :delete,
          class: "btn btn-danger btn-sm", data: {confirm: t("admin.confirm_del")} %>
      <% end %>
    </td>
  </tr>
<% end %>
